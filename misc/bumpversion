#!/bin/sh

if [ $# != 2 ]; then
  echo 1>&2 "USAGE: bumpversion <versionfile> <changelog>"
  exit 1
fi

version="$(cat "$1")"
echo "wollmux (${version}-0lhm1) wanderer; urgency=low

    * Version bump.

 -- Felix WollMux <wollmux-general@lists.forge.osor.eu>  Thu, 24 Nov 2016 08:51:22 +0100
" >"$2"
